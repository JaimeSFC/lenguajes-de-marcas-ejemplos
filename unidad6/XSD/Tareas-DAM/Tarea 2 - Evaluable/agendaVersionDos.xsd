	<xs:complexType name="diaType">
		<xs:sequence>
		<xs:element name="dia" type="xs:int"/>
		<xs:element name="mes" type="xs:int"/>
		<xs:element name="año" type="xs:int"/>
		</xs:sequence>
		</xs:complexType>

		<xs:complexType name="dia">
			<xs:sequence>
				<xs:element name="diaType"/>
